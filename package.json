{
    "name": "unsaved-files-vscode",
    "displayName": "Unsaved Files",
    "description": "Easy access to unsaved files for vscode.",
    "icon": "images/infohex.128.png",
    "version": "0.0.0",
    "publisher": "wraith13",
    "license": "SEE LICENSE IN LICENSE_1_0.txt",
    "engines": {
        "vscode": "^1.9.1"
    },
    "bugs": {
        "url": "https://github.com/wraith13/unsaved-files-vscode/issues"
    },
    "homepage": "https://github.com/wraith13/unsaved-files-vscode/blob/master/README.md",
    "repository": {
        "type": "git",
        "url": "https://github.com/wraith13/unsaved-files-vscode.git"
    },
    "categories": [
        "Other"
    ],
    "keywords": [
        "vscode",
        "system",
        "system information",
        "extensions",
        "extensions list"
    ],
    "activationEvents": [
        "onCommand:unsaved-files-vscode.showSystemInformation"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "commands": [
            {
                "command": "unsaved-files-vscode.showSystemInformation",
                "title": "Show",
                "category": "Unsaved Files"
            }
        ],
        "configuration": [
            {
                "title": "Unsaved Files",
                "properties": {
                    "unsaved-files.hideItems": {
                        "type": "array",
                        "items": {
                            "type": "string"
                        },
                        "default": [
                        ],
                        "description": "list of hide items ( see README )"
                    }
                }
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "dependencies": {},
    "devDependencies": {
        "typescript": "^2.9.2",
        "vscode": "^1.1.21",
        "mocha": "^2.3.3",
        "@types/node": "^6.0.40",
        "@types/mocha": "^2.2.32"
    }
}